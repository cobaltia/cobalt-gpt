{
	"name": "cobalt-gpt",
	"version": "0.1.0",
	"main": "dist/bot.js",
	"type": "module",
	"imports": {
		"#lib/*": "./dist/lib/*.js",
		"#lib/types": "./dist/lib/types/index.js",
		"#lib/structures": "./dist/lib/structures/index.js",
		"#lib/structures/listeners": "./dist/lib/structures/listeners/index.js",
		"#root/*": "./dist/*.js",
		"#utils/*": "./dist/lib/utils/*.js",
		"#root/interactions": "./dist/interactions/index.js",
		"#lib/database": "./dist/lib/database/index.js"
	},
	"scripts": {
		"test:typescript": "tsc -b src",
		"test:prettier": "prettier --check .",
		"clean": "node scripts/clean.mjs",
		"build": "tsc -b src",
		"start": "NODE_ENV=production node --enable-source-maps dist/bot.js",
		"start:dev": "node --enable-source-maps dist/bot.js",
		"deploy:commands": "node --enable-source-maps dist/deploy.js",
		"dev": "pnpm run build && pnpm run start:dev",
		"prettier": "prettier --write .",
		"lint": "eslint --fix --ext ts src"
	},
	"repository": "https://github.com/cobaltia/cobalt-gpt.git",
	"author": "JuanPablo2655 <personal@isidrocasiano.com>",
	"license": "MIT",
	"private": true,
	"dependencies": {
		"@discordjs/voice": "^0.19.0",
		"@sapphire/discord.js-utilities": "^7.3.3",
		"@sapphire/duration": "^1.2.0",
		"@sapphire/framework": "^5.3.6",
		"@sapphire/ratelimits": "^2.4.11",
		"@sapphire/timestamp": "^1.0.5",
		"@sapphire/utilities": "^3.18.2",
		"@skyra/env-utilities": "^2.0.1",
		"@snazzah/davey": "^0.1.9",
		"discord-api-types": "^0.38.24",
		"discord.js": "^14.22.1",
		"globby": "^14.1.0",
		"openai": "^5.20.2"
	},
	"devDependencies": {
		"@types/lodash.merge": "^4.6.9",
		"@types/node": "^24.4.0",
		"eslint": "^9.35.0",
		"eslint-config-neon": "^0.2.7",
		"lodash.merge": "^4.6.2",
		"prettier": "^3.6.2",
		"tslib": "^2.8.1",
		"typescript": "^5.9.2"
	}
}
